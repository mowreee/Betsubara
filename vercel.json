{
    "version": 2,
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "betsubara-backend/src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "betsubara-backend/src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "client/dist/$1"
        }
    ]
}